  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "bun@1.2.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "nitrogen": "^0.29.6",
    "react-native-nitro-modules": "^0.29.6",
    "semver": "^7.7.2"
  },
  "resolutions": {
    "semver": "^7.6.3"
  }
